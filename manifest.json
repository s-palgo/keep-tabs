{
    "manifest_version": 3,
    "description": "Saves your tabs/windows. Organizes saved items by date saved. Offers functionality to view, rename, and remove saved items.",
    "version": "1.0",
    "name": "Keep Tabs",
    "action": {
        "default_popup": "index.html", 
        "default_icon": {
            "16": "tabs_logo_16x16.png",
            "32": "tabs_logo_32x32.png",
            "48": "tabs_logo_48x48.png",
            "128": "tabs_logo_128x128.png"
        }
    },
    "permissions": [
        "tabs"
    ], 
    "icons": {
        "16": "tabs_logo_16x16.png",
        "32": "tabs_logo_32x32.png",
        "48": "tabs_logo_48x48.png",
        "128": "tabs_logo_128x128.png"
    }
}